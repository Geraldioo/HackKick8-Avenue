<%- include('components/header.ejs') %>
<head>
  <style>
      
      body,
      h1,
      h2,
      h3,
      p,
      ul,
      li {
          margin: 0;
          padding: 0;
      }

      body {
          font-family: 'Arial', sans-serif; 
          background-color: #131210;
      }

      .container {
          max-width: 800px; 
          margin: 0 auto;
          padding: 20px;
      }

      .carousel {
          position: relative;
          margin-top: 20px; 
      }

      .carousel-indicators {
          position: absolute;
          bottom: 10px;
          left: 50%;
          transform: translateX(-50%);
      }

      .carousel-indicators button {
          background-color: #007BFF; 
          border: none;
          width: 10px;
          height: 10px;
          margin: 0 5px;
          border-radius: 50%;
          cursor: pointer;
      }

      .carousel-indicators button.active {
          background-color: #0056b3; 
      }

      .carousel-inner {
          position: relative;
      }

      .carousel-item {
          display: none;
          width: 100%;

      }

      .carousel-item.active {
          display: block;
          width: 100%;
      }

      .carousel-control-prev,
      .carousel-control-next {
          position: absolute;
          top: 50%;
          transform: translateY(-50%);
          width: 30px;
          height: 30px;
          border: none;
          background-color: #007BFF; 
          border-radius: 50%;
          cursor: pointer;
          color: #fff;
      }

      .carousel-control-prev-icon,
      .carousel-control-next-icon {
          font-size: 1.5rem; 
          line-height: 1.7;
      }

      .carousel-control-prev {
          left: 10px;
      }

      .carousel-control-next {
          right: 10px;
      }

     
      @media (max-width: 600px) {
          .container {
              max-width: 100%; 
              padding: 10px;
          }
      }

      @media (max-width: 768px) {
          .carousel-control-prev,
          .carousel-control-next {
              width: 100%;
              height: 20px;
              font-size: 1rem;
          }
      }
      .imgPublic{
        width: 100%;
      }
  </style>
</head>
